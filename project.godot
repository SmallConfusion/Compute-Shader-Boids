; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Boids"
run/main_scene="res://boids/node_2d.tscn"
config/features=PackedStringArray("4.2")
run/max_fps=60
config/icon="res://icon.svg"

[autoload]

DebugCam="*res://addons/debug_camera/scripts/DebugCamAutoload.gd"

[debug]

settings/stdout/print_fps=true

[display]

window/size/viewport_width=1200
window/size/viewport_height=1200

[editor_plugins]

enabled=PackedStringArray("res://addons/debug_camera/plugin.cfg")
